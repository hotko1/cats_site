{{ attach_library('ar/ar_block') }}
{% if forms %}
  <div class="form_wrapper">
    {{ forms }}
  </div>
{% endif %}

<div class="outputs_inner">
  {% for card in tables %}
  <div class="outputs_item">
    <div class="top">
      <p class="names">
        {{ card['name'] }}
      </p>
      <p class="emails">
        {{ card['email_user'] }}
      </p>
      <p class="times">
        {{ card['time'] }}
      </p>
    </div>
    <div class="middle">
      {{ card['fid'] }}
    </div>
    {% if user.hasPermission('ROLE_ADMIN') %}
    <div class="bottom">
      {{ card['delete'] }}
      {{ card['edit'] }}
    </div>
    {% endif %}
  </div>
  {% endfor %}
</div>
